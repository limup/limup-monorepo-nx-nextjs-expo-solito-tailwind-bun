{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "@limup-monorepo-nx-nextjs-expo-solito-tailwind-bun/features",
  "projectType": "library",
  "sourceRoot": "packages/features/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "cwd": "packages/features",
        "command": "bun run build"
      },
      "configurations": {
        "watch": {
          "command": "bun run build:watch"
        }
      }
    }
  }
}

